<h1>Admin Dashboard</h1>
<p>Active sessions:</p>
<table class="table">
  <thead>
    <tr><th>Session ID</th><th>User ID</th><th>Action</th></tr>
  </thead>
  <tbody>
    <% sessions.forEach(function(s) { %>
      <tr>
        <td><%= s.id %></td>
        <td><%= s.userId || "unknown" %></td>
        <td>
          <form method="POST" action="/admin/revoke/<%= s.id %>">
            <button type="submit" class="btn btn-danger btn-sm">Revoke</button>
          </form>
        </td>
      </tr>
    <% }) %>
  </tbody>
</table>
